#!/usr/bin/expect

set timeout 20


spawn ./ask_me.sh

expect {
    "What is your name?" {
        send "Jacob\r"
        exp_continue
    }
    "How many CPUs do you want?" {
        send "4\r"
    }
    timeout {
        puts "Interaction timed out."
        exit 1
    }
    eof {
        puts "Unexpected end of program."
        exit 1
    }
}

interact
